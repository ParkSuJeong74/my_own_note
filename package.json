  "scripts": {
    "start:web": "nx dev web --port 3000",
    "start:api": "nx serve api",
    "start:docs": "nx serve docs",
    "start:mobile": "nx start mobile --port 8000 --clear",
    "start:desktop": "nx serve desktop --port 7000",
