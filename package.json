  "private": true,
  "scripts": {
    "start:web": "nx dev web --port 3000",
    "start:api": "nx serve api --port 4000 --watch",
    "start:mobile": "nx start mobile --port 8000 --clear",
    "start:desktop": "nx serve desktop --port 7000",
    "start:all": "concurrently --names \"WEB,API,MOBILE,DESKTOP\" --prefix-colors \"cyan,green,magenta,blue\" \"npm run start:web\" \"npm run start:api\" \"npm run start:mobile\" \"npm run start:desktop\"",
    "build:web": "nx build web",
    "build:api": "nx build api",
    "build:mobile": "nx build mobile",
    "build:desktop": "nx build desktop",
    "build:all": "nx run-many --target=build --all",
    "test": "nx test",
    "test:all": "nx run-many --target=test --all",
    "test:affected": "nx affected --target=test",
    "lint": "nx lint",
    "lint:all": "nx run-many --target=lint --all",
    "lint:affected": "nx affected --target=lint",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "graph": "nx graph",
    "dep-graph": "nx dep-graph",
    "affected:build": "nx affected --target=build",
    "affected:test": "nx affected --target=test",
    "clean": "nx reset",
    "postinstall": "electron-builder install-app-deps",
    "nxe:build:frontend": "nx build web",
    "nxe:build:backend": "nx build desktop",
    "nxe:serve:frontend": "nx serve web",
    "nxe:serve:backend": "nx serve desktop",
    "nxe:test:frontend": "nx test web",
    "nxe:test:backend": "nx test desktop",
    "nxe:package:app": "nx run desktop:make --prepackageOnly",
    "nxe:make:app": "nx run desktop:make"
  },
